SubjectArea: ConceptElement @ Model + TopSpace
    +Domain
    +Entity
    .prefix: String? = null

Domain: ConceptElement @ SubjectArea
    .abstract: Boolean = false

Entity: ConceptElement @ SubjectArea
    +Attribute
    +InvEntry

Attribute: ConceptElement @ Entity
    .domain --> Domain
    .abstract: Boolean = false
    .autoincrement: Boolean = false

InvEntry: ConceptElement @ Entity
    .attribute ->> Attribute
    .unique: Boolean = false
    .primary: Boolean = false

